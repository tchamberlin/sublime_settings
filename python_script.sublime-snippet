#! /usr/bin/env python
# -*- coding: utf-8 -*-

"""docstring"""


import argparse


def main():
    args = parse_args()


def parse_args():
    parser = argparse.ArgumentParser(
        formatter_class=argparse.ArgumentDefaultsHelpFormatter
    )
    parser.add_argument()
    return parser.parse_args()


if __name__ == '__main__':
    main()

